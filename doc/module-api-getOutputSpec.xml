<refentry id="module-api-getOutputSpec">

  <refnamediv>
    <refname>getOutputSpec</refname>
    <refpurpose>query the specification of an output</refpurpose>
  </refnamediv>

  <refsynopsisdiv>
    <funcsynopsis>
      <funcprototype>
        <funcdef>char* <function>getOutputSpec</function></funcdef>
        <paramdef>int <parameter>index</parameter></paramdef>
